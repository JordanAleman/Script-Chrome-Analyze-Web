{
    "name": "Style Analyzer",
    "version": "1.0",
    "description": "Analyze and display styles from a webpage",
    "manifest_version": 3,
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Style Analyzer"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
